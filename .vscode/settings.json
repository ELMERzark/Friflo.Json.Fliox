{
	"files.exclude": {
		"**/*.meta": true,
		"**/*.sync.resp.json": true,
		"**/*.sync.resp.md": true,
    },
    "cSpell.words": [
        "AGPL",
        "appsettings",
        "Arity",
        "Fliox",
        "HTTPAPI",
        "JSONB",
        "jsxdev",
        "kotlinx",
        "Linq",
        "NETFX",
        "netsh",
        "noopener",
        "noreferrer",
        "nullary",
        "Postgres",
        "Praetz",
        "SIMD",
        "Sublicensing",
        "uglifyjs",
        "Ullrich",
        "upsert",
        "Upserts",
        "vbar",
        "WIPO"
    ],
    "cSpell.enableFiletypes": [
        "http"
    ],
    "dotnet-test-explorer.testProjectPath": "Json.Tests",
    "editor.detectIndentation": false,
    "json.schemas": [
        { "fileMatch": ["/Json.Tests/assets~/DB/UserStore/roles/*.json"],                   "url": "./Json.Tests/assets~/Schema/JSON/UserStore/Friflo.Json.Fliox.Hub.DB.UserAuth.Role.json" },
        { "fileMatch": ["/Json.Tests/assets~/DB/UserStore/credentials/*.json"],             "url": "./Json.Tests/assets~/Schema/JSON/UserStore/Friflo.Json.Fliox.Hub.DB.UserAuth.UserCredential.json" },
        { "fileMatch": ["/Json.Tests/assets~/DB/UserStore/permissions/*.json"],             "url": "./Json.Tests/assets~/Schema/JSON/UserStore/Friflo.Json.Fliox.Hub.DB.UserAuth.UserPermission.json" },

        { "fileMatch": ["/Json.Tests/assets~/DB/PocStore/articles/*.json"],                 "url": "./Json.Tests/assets~/Schema/JSON/PocStore/UnitTest.Fliox.Client.Article.json" },
        { "fileMatch": ["/Json.Tests/assets~/DB/PocStore/customers/*.json"],                "url": "./Json.Tests/assets~/Schema/JSON/PocStore/UnitTest.Fliox.Client.Customer.json" },
        { "fileMatch": ["/Json.Tests/assets~/DB/PocStore/employees/*.json"],                "url": "./Json.Tests/assets~/Schema/JSON/PocStore/UnitTest.Fliox.Client.Employee.json" },
        { "fileMatch": ["/Json.Tests/assets~/DB/PocStore/orders/*.json"],                   "url": "./Json.Tests/assets~/Schema/JSON/PocStore/UnitTest.Fliox.Client.Order.json" },
        { "fileMatch": ["/Json.Tests/assets~/DB/PocStore/producers/*.json"],                "url": "./Json.Tests/assets~/Schema/JSON/PocStore/UnitTest.Fliox.Client.Producer.json" },
        { "fileMatch": ["/Json.Tests/assets~/DB/PocStore/types/*.json"],                    "url": "./Json.Tests/assets~/Schema/JSON/PocStore/UnitTest.Fliox.Client.TestType.json" },

        { "fileMatch": ["/Json.Tests/assets~/DB/EntityIdStore/guidEntities/*.json"],        "url": "./Json.Tests/assets~/Schema/JSON/EntityIdStore/UnitTest.Fliox.Client.GuidEntity.json" },
        { "fileMatch": ["/Json.Tests/assets~/DB/EntityIdStore/intEntities/*.json"],         "url": "./Json.Tests/assets~/Schema/JSON/EntityIdStore/UnitTest.Fliox.Client.IntEntity.json" },
        { "fileMatch": ["/Json.Tests/assets~/DB/EntityIdStore/longEntities/*.json"],        "url": "./Json.Tests/assets~/Schema/JSON/EntityIdStore/UnitTest.Fliox.Client.LongEntity.json" },
        { "fileMatch": ["/Json.Tests/assets~/DB/EntityIdStore/shortEntities/*.json"],       "url": "./Json.Tests/assets~/Schema/JSON/EntityIdStore/UnitTest.Fliox.Client.ShortEntity.json" },
        { "fileMatch": ["/Json.Tests/assets~/DB/EntityIdStore/byteEntities/*.json"],        "url": "./Json.Tests/assets~/Schema/JSON/EntityIdStore/UnitTest.Fliox.Client.ByteEntity.json" },
        { "fileMatch": ["/Json.Tests/assets~/DB/EntityIdStore/customIdEntities/*.json"],    "url": "./Json.Tests/assets~/Schema/JSON/EntityIdStore/UnitTest.Fliox.Client.CustomIdEntity.json" },
        { "fileMatch": ["/Json.Tests/assets~/DB/EntityIdStore/customIdEntities2/*.json"],   "url": "./Json.Tests/assets~/Schema/JSON/EntityIdStore/UnitTest.Fliox.Client.CustomIdEntity2.json" },
        { "fileMatch": ["/Json.Tests/assets~/DB/EntityIdStore/entityRefs/*.json"],          "url": "./Json.Tests/assets~/Schema/JSON/EntityIdStore/UnitTest.Fliox.Client.EntityRefs.json" },

        { "fileMatch": ["/**/*.sync.json"],        "url": "./Json.Tests/assets~/Schema/JSON/Protocol/Friflo.Json.Fliox.Hub.Protocol.ProtocolRequest.json"  },
        { "fileMatch": ["/**/*.sync.resp.json"],   "url": "./Json.Tests/assets~/Schema/JSON/Protocol/Friflo.Json.Fliox.Hub.Protocol.ProtocolResponse.json" }        
        
    ],
    "editor.lineNumbers": "off",
    "cSpell.ignorePaths": [
        "package-lock.json",
        "node_modules",
        "vscode-extension",
        ".git/objects",
        ".vscode",
        ".vscode-insiders",
        "Json.Tests/www/example-requests/**/*.*"
    ]
}