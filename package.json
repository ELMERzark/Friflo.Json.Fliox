{
  "name": "friflo-fliox",
  "version": "0.1.0",
  "author": "Ullrich Praetz",
  "scripts": {
    "all": "npm run www:tsc && npm run schema:tsc && npm run schema:run",

    "schema:tsc": "tsc --project Json.Tests/assets~/Schema/Typescript",
    "schema:run": "npm run run -w Json.Tests/assets~/Schema/Typescript",

    "www:tsc":       "tsc --project Json/Fliox.Hub.Explorer/www~/src",
    "www:tsc-watch": "tsc --project Json/Fliox.Hub.Explorer/www~/src -w",

    "postinstall": "node scripts/postinstall.js"
  },
  "devDependencies": {
    "ajv": "^8.6.2",
    "typescript": "^4.3.5",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "eslint": "^7.32.0",
    "fswin": "^3.21.1015"
  },
  "dependencies": {},
  "workspaces": [
      "Json/Fliox.Hub.Explorer/www~/src",
      "Json.Tests/assets~/Schema/Typescript"
  ]
}
