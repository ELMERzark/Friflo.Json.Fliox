<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="Friflo.Fliox.Editor.UI.MainWindow"
        Background="Transparent"
        Title="Fliox Editor">
        <DockPanel >
                <Rectangle Fill="#fcc" Height="100" DockPanel.Dock="Top"/>
                <Rectangle Fill="#ccf"  Width="100" DockPanel.Dock="Left"/>
                <Rectangle Fill="#cfc" Height="100" DockPanel.Dock="Bottom"/>
                <Rectangle Fill="#ffc" Width="100" DockPanel.Dock="Right"/>
                <Rectangle Fill="Transparent" />
        </DockPanel>
</Window>
