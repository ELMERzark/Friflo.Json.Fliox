{
	"files.exclude": {
		"**/*.meta": true
    },
    "cSpell.words": [
        "JSONB",
        "Linq",
        "Postgres",
        "Praetz",
        "SIMD",
        "Ullrich",
        "netsh"
    ],
    "cSpell.enableFiletypes": [
        "http"
    ],
    "dotnet-test-explorer.testProjectPath": "Json.Tests",
    "editor.detectIndentation": false,
    "json.schemas": [
        { "fileMatch": ["/Json.Tests/assets/Graph/auth/Role/*.json"],           "url": "./Json.Tests/assets/schema/Role.json" },
        { "fileMatch": ["/Json.Tests/assets/Graph/auth/UserCredential/*.json"], "url": "./Json.Tests/assets/schema/UserCredential.json" },
        { "fileMatch": ["/Json.Tests/assets/Graph/auth/UserPermission/*.json"], "url": "./Json.Tests/assets/schema/UserPermission.json" }
    ]
}