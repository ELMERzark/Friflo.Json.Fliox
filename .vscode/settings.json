        { "fileMatch": ["/Json.Tests/assets/Graph/db/Article/*.json"],  "url": "./Json.Tests/assets/Schema/JSON/PocStore/UnitTest.Flow.Graph.Article.json" },
        { "fileMatch": ["/Json.Tests/assets/Graph/db/Customer/*.json"], "url": "./Json.Tests/assets/Schema/JSON/PocStore/UnitTest.Flow.Graph.Customer.json" },
        { "fileMatch": ["/Json.Tests/assets/Graph/db/Employee/*.json"], "url": "./Json.Tests/assets/Schema/JSON/PocStore/UnitTest.Flow.Graph.Employee.json" },
        { "fileMatch": ["/Json.Tests/assets/Graph/db/Order/*.json"],    "url": "./Json.Tests/assets/Schema/JSON/PocStore/UnitTest.Flow.Graph.Order.json" },
